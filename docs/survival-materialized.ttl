#    Object Properties
#################################################################

###  http://schema.org/location
<http://schema.org/location> rdf:type owl:ObjectProperty ;
                             owl:equivalentProperty <http://www.wikidata.org/prop/direct/P706> ;
                             rdf:type owl:TransitiveProperty ;
                             rdfs:label "location" .


###  http://schema.org/parentOrganization
<http://schema.org/parentOrganization> rdf:type owl:ObjectProperty ;
                                       owl:equivalentProperty <http://www.wikidata.org/prop/direct/P749> ;
                                       owl:inverseOf <http://schema.org/subOrganization> ,
                                                     <http://www.wikidata.org/prop/direct/P355> ,
                                                     :hasBranch ;
                                       rdfs:label "parentOrganization" .


###  http://schema.org/subOrganization
<http://schema.org/subOrganization> rdf:type owl:ObjectProperty ;
                                    owl:equivalentProperty <http://www.wikidata.org/prop/direct/P355> ;
                                    owl:inverseOf <http://www.wikidata.org/prop/direct/P749> ,
                                                  :branchOf ;
                                    rdfs:label "subOrganization" .


###  http://www.ontologydesignpatterns.org/cp/owl/coparticipation.owl#coparticipatesWith
<http://www.ontologydesignpatterns.org/cp/owl/coparticipation.owl#coparticipatesWith> rdf:type owl:ObjectProperty ;
                                                                                      owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/coparticipation.owl#coparticipatesWith> ;
                                                                                      rdf:type owl:SymmetricProperty ;
                                                                                      rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/coparticipation.owl> ;
                                                                                      rdfs:label "coparticipates with"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant
<http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant> rdf:type owl:ObjectProperty ;
                                                                                owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn> ;
                                                                                rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/participation.owl> ;
                                                                                rdfs:label "has participant"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn
<http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn> rdf:type owl:ObjectProperty ;
                                                                                 rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/participation.owl> ;
                                                                                 rdfs:label "is participant in"@en .


###  http://www.wikidata.org/prop/direct/P1706
<http://www.wikidata.org/prop/direct/P1706> rdf:type owl:ObjectProperty ;
                                            rdfs:label "insieme a"@it ,
                                                       "together with"@en .


###  http://www.wikidata.org/prop/direct/P355
<http://www.wikidata.org/prop/direct/P355> rdf:type owl:ObjectProperty ;
                                           owl:equivalentProperty :hasBranch ;
                                           owl:inverseOf <http://www.wikidata.org/prop/direct/P749> ,
                                                         :branchOf ;
                                           rdfs:label "filiali"@it ,
                                                      "subsidiary"@en .


###  http://www.wikidata.org/prop/direct/P706
<http://www.wikidata.org/prop/direct/P706> rdf:type owl:ObjectProperty ;
                                           owl:equivalentProperty :locatedIn ;
                                           rdf:type owl:TransitiveProperty ;
                                           rdfs:label "located in/on physical feature"@en ,
                                                      "luogo geografico"@it .


###  http://www.wikidata.org/prop/direct/P749
<http://www.wikidata.org/prop/direct/P749> rdf:type owl:ObjectProperty ;
                                           owl:equivalentProperty :branchOf ;
                                           owl:inverseOf :hasBranch ;
                                           rdfs:label "ente controllante"@it ,
                                                      "parent organization"@en .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#addressedBy
:addressedBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :addresses ;
             rdfs:comment "Links a threat to a campaign addressing it"@en ;
             rdfs:label "Addressed by"@en ,
                        "Affrontata da"@it .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#addresses
:addresses rdf:type owl:ObjectProperty ;
           rdfs:comment "Links a campaign to a threat that it addresses"@en ;
           rdfs:label "Addresses"@en ,
                      "Affronta"@it .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#branchOf
:branchOf rdf:type owl:ObjectProperty ;
          owl:equivalentProperty [ owl:inverseOf <http://schema.org/subOrganization> ;
                                   owl:equivalentProperty [ owl:inverseOf <http://www.wikidata.org/prop/direct/P355> ;
                                                            owl:equivalentProperty [ owl:inverseOf :hasBranch
                                                                                   ]
                                                          ]
                                 ] ;
          owl:inverseOf :hasBranch ;
          rdfs:comment "Links a branch organization to its head organization"@en ;
          rdfs:label "Branch of"@en ,
                     "Ufficio di"@it .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#cohabitsWith
:cohabitsWith rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://www.wikidata.org/prop/direct/P1706> ,
                                 [ owl:inverseOf <http://www.wikidata.org/prop/direct/P1706>
                                 ] ;
              owl:inverseOf :cohabitsWith ;
              rdf:type owl:SymmetricProperty ;
              rdfs:comment "Links a tribe to another tribe it shares a habitat with"@en ;
              rdfs:label "Coabita con"@it ,
                         "Cohabits with"@en .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#collaboratesWith
:collaboratesWith rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/coparticipation.owl#coparticipatesWith> ,
                                     [ owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/coparticipation.owl#coparticipatesWith>
                                     ] ;
                  owl:inverseOf :collaboratesWith ;
                  rdf:type owl:SymmetricProperty ;
                  rdfs:comment "Links an organization with another organization it collaborates with in one or more campaigns"@en ;
                  rdfs:label "Collabora con"@it ,
                             "Collaborates with"@en .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#explicitlyHelps
:explicitlyHelps rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :helps ,
                                    [ owl:inverseOf :helpedBy
                                    ] ;
                 rdfs:comment "Links a campaign to a tribe it explicitly helps"@en ;
                 rdfs:label "Aiuta esplicitamente"@it ,
                            "Explicitly helps"@en .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#hasBranch
:hasBranch rdf:type owl:ObjectProperty ;
           owl:equivalentProperty [ owl:inverseOf <http://schema.org/parentOrganization> ;
                                    owl:equivalentProperty [ owl:inverseOf <http://www.wikidata.org/prop/direct/P749> ;
                                                             owl:equivalentProperty [ owl:inverseOf :branchOf
                                                                                    ]
                                                           ]
                                  ] ;
           rdfs:comment "Links an organization to one of its branches"@en ;
           rdfs:label "Ha ufficio"@it ,
                      "Has branch"@en .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#helpedBy
:helpedBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :helps ;
          rdf:type owl:AsymmetricProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:comment "Links a tribe to a campaign it is helped by"@en ;
          rdfs:label "Aiutata da"@it ,
                     "Helped by"@en .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#helps
:helps rdf:type owl:ObjectProperty ,
                owl:AsymmetricProperty ,
                owl:IrreflexiveProperty ;
       rdfs:comment "Links a campaign to a tribe it implicitly helps"@en ;
       rdfs:label "Aiuta"@it ,
                  "Helps"@en .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#hosts
:hosts rdf:type owl:ObjectProperty ;
       owl:inverseOf :livesIn ;
       rdfs:comment "Links a habitat to a tribe it hosts"@en ;
       rdfs:label "Hosts"@en ,
                  "Ospita"@it .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#leads
:leads rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn> ,
                          [ owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant>
                          ] ;
       owl:inverseOf :ledBy ;
       rdfs:comment "Links an organization to a campaign it conducts"@en ;
       rdfs:label "Conduce"@it ,
                  "Leads"@en .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#ledBy
:ledBy rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant> ,
                          [ owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn>
                          ] ;
       rdfs:comment "Links a campaign to an organization it is conducted by"@en ;
       rdfs:label "Condotta da"@it ,
                  "Led by"@en .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#livesIn
:livesIn rdf:type owl:ObjectProperty ;
         rdfs:comment "Links a tribe to a habitat it lives in"@en ;
         rdfs:label "Abita in"@it ,
                    "Lives in"@en .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#locatedIn
:locatedIn rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           rdfs:comment "Links a country or a habitat or an organization to a location it is located in"@en ;
           rdfs:label "Located in"@en ,
                      "Situato in"@it .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#threatenedBy
:threatenedBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :threatens ;
              rdfs:comment "Links a tribe to a threat it is endangered by"@en ;
              rdfs:label "Minacciata da"@it ,
                         "Threatened by"@en .


###  https://acontenti.github.io/progetto-modsem/survival.ttl#threatens
:threatens rdf:type owl:ObjectProperty ;
           rdfs:comment "Links a threat to a tribe it endangers"@en ;
           rdfs:label "Minaccia"@it ,
                      "Threatens"@en .


