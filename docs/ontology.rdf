<?xml version="1.0"?>
<rdf:RDF xmlns="https://acontenti.github.io/progetto-modsem/survival.ttl#"
     xml:base="https://acontenti.github.io/progetto-modsem/survival.ttl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:survival="https://acontenti.github.io/progetto-modsem/survival.ttl#">
    <owl:Ontology rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl">
        <dc:creator>Alessandro Contenti</dc:creator>
        <dc:date>2022-02-04</dc:date>
        <dc:description xml:lang="en">An ontology to describe the operations of the Survival International NGO</dc:description>
        <dc:title xml:lang="en">Survival International Ontology</dc:title>
        <vann:preferredNamespacePrefix>survival</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://acontenti.github.io/progetto-modsem/survival.ttl#</vann:preferredNamespaceUri>
        <rdfs:label>Survival</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/elements/1.1/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/date -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/date"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespacePrefix -->


    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespaceUri -->


    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#emailAddress -->


    <rdfs:Datatype rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#emailAddress">
        <rdfs:comment xml:lang="en">A datatype for an e-mail address</rdfs:comment>
        <rdfs:label xml:lang="en">E-mail</rdfs:label>
        <rdfs:label xml:lang="it">E-mail</rdfs:label>
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>[^@]+@[^\.]+\..+</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdfs:Datatype>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#addressedBy -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#addressedBy">
        <owl:inverseOf rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#addresses"/>
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Threat"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Campaign"/>
        <rdfs:comment xml:lang="en">Links a threat to a campaign addressing it</rdfs:comment>
        <rdfs:label xml:lang="en">Addressed by</rdfs:label>
        <rdfs:label xml:lang="it">Affrontata da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#addresses -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#addresses">
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Campaign"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Threat"/>
        <rdfs:comment xml:lang="en">Links a campaign to a threat that it addresses</rdfs:comment>
        <rdfs:label xml:lang="en">Addresses</rdfs:label>
        <rdfs:label xml:lang="it">Affronta</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#branchOf -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#branchOf">
        <owl:inverseOf rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#hasBranch"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#BranchOrganization"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Organization"/>
        <rdfs:comment xml:lang="en">Links a branch organization to its head organization</rdfs:comment>
        <rdfs:label xml:lang="en">Branch of</rdfs:label>
        <rdfs:label xml:lang="it">Ufficio di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#cohabitsWith -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#cohabitsWith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Tribe"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Tribe"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#livesIn"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#livesIn"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Links a tribe to another tribe it shares a habitat with</rdfs:comment>
        <rdfs:label xml:lang="it">Coabita con</rdfs:label>
        <rdfs:label xml:lang="en">Cohabits with</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#explicitlyHelps -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#explicitlyHelps">
        <rdfs:subPropertyOf rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#helps"/>
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#LocalCampaign"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Tribe"/>
        <rdfs:comment xml:lang="en">Links a campaign to a tribe it explicitly helps</rdfs:comment>
        <rdfs:label xml:lang="it">Aiuta esplicitamente</rdfs:label>
        <rdfs:label xml:lang="en">Explicitly helps</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#hasBranch -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#hasBranch">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Organization"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#BranchOrganization"/>
        <rdfs:comment xml:lang="en">Links an organization to one of its branches</rdfs:comment>
        <rdfs:label xml:lang="it">Ha ufficio</rdfs:label>
        <rdfs:label xml:lang="en">Has branch</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#helpedBy -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#helpedBy">
        <owl:inverseOf rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#helps"/>
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Tribe"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Campaign"/>
        <rdfs:comment xml:lang="en">Links a tribe to a campaign it is helped by</rdfs:comment>
        <rdfs:label xml:lang="it">Aiutata da</rdfs:label>
        <rdfs:label xml:lang="en">Helped by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#helps -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#helps">
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Campaign"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Tribe"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#addresses"/>
            <rdf:Description rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#threatens"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Links a campaign to a tribe it implicitly helps</rdfs:comment>
        <rdfs:label xml:lang="it">Aiuta</rdfs:label>
        <rdfs:label xml:lang="en">Helps</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#hosts -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#hosts">
        <owl:inverseOf rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#livesIn"/>
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Habitat"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Tribe"/>
        <rdfs:comment xml:lang="en">Links a habitat to a tribe it hosts</rdfs:comment>
        <rdfs:label xml:lang="en">Hosts</rdfs:label>
        <rdfs:label xml:lang="it">Ospita</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#leads -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#leads">
        <owl:inverseOf rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#ledBy"/>
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Organization"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Campaign"/>
        <rdfs:comment xml:lang="en">Links an organization to a campaign it conducts</rdfs:comment>
        <rdfs:label xml:lang="it">Conduce</rdfs:label>
        <rdfs:label xml:lang="en">Leads</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#ledBy -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#ledBy">
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Campaign"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Organization"/>
        <rdfs:comment xml:lang="en">Links a campaign to an organization it is conducted by</rdfs:comment>
        <rdfs:label xml:lang="it">Condotta da</rdfs:label>
        <rdfs:label xml:lang="en">Led by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#livesIn -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#livesIn">
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Tribe"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Habitat"/>
        <rdfs:comment xml:lang="en">Links a tribe to a habitat it lives in</rdfs:comment>
        <rdfs:label xml:lang="it">Abita in</rdfs:label>
        <rdfs:label xml:lang="en">Lives in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#locatedIn -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#locatedIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#Country"/>
                    <rdf:Description rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#Habitat"/>
                    <rdf:Description rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#Organization"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Location"/>
        <rdfs:comment xml:lang="en">Links a country or a habitat or an organization to a location it is located in</rdfs:comment>
        <rdfs:label xml:lang="en">Located in</rdfs:label>
        <rdfs:label xml:lang="it">Situato in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#threatenedBy -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#threatenedBy">
        <owl:inverseOf rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#threatens"/>
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Tribe"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Threat"/>
        <rdfs:comment xml:lang="en">Links a tribe to a threat it is endangered by</rdfs:comment>
        <rdfs:label xml:lang="it">Minacciata da</rdfs:label>
        <rdfs:label xml:lang="en">Threatened by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#threatens -->


    <owl:ObjectProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#threatens">
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Threat"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Tribe"/>
        <rdfs:comment xml:lang="en">Links a threat to a tribe it endangers</rdfs:comment>
        <rdfs:label xml:lang="it">Minaccia</rdfs:label>
        <rdfs:label xml:lang="en">Threatens</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#campaignPage -->


    <owl:DatatypeProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#campaignPage">
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Campaign"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment xml:lang="en">URL of the web page of the campaign</rdfs:comment>
        <rdfs:label xml:lang="en">Campaign web page</rdfs:label>
        <rdfs:label xml:lang="it">Pagina web della campagna</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#contacted -->


    <owl:DatatypeProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#contacted">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Tribe"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Whether a tribe has been contacted or not: defines its uncontacted status</rdfs:comment>
        <rdfs:label xml:lang="en">Contacted tribe</rdfs:label>
        <rdfs:label xml:lang="it">Tribù contattata</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#email -->


    <owl:DatatypeProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#email">
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Organization"/>
        <rdfs:range rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#emailAddress"/>
        <rdfs:comment xml:lang="en">An organization e-mail address</rdfs:comment>
        <rdfs:label xml:lang="it">Indirizzo e-mail dell&apos;organizzazione</rdfs:label>
        <rdfs:label xml:lang="en">Organization e-mail address</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#habitatType -->


    <owl:DatatypeProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#habitatType">
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Habitat"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>desert</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>forest</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>island</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Defines the type of an habitat, can be either desert, forest or island. Can be used twice to define an intersection, like: forest + island = forested island</rdfs:comment>
        <rdfs:label xml:lang="en">Habitat type</rdfs:label>
        <rdfs:label xml:lang="it">Tipo di habitat</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#name -->


    <owl:DatatypeProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#name">
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Tribe"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The official name of a tribe or indigenous people</rdfs:comment>
        <rdfs:label xml:lang="it">Nome della tribù</rdfs:label>
        <rdfs:label xml:lang="en">Tribe name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://acontenti.github.io/progetto-modsem/survival.ttl#website -->


    <owl:DatatypeProperty rdf:about="https://acontenti.github.io/progetto-modsem/survival.ttl#website">
        <rdfs:domain rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Organization"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment xml:lang="en">URL of the website of an organization</rdfs:comment>
        <rdfs:label xml:lang="en">Organization website</rdfs:label>
        <rdfs:label xml:lang="it">Sito web dell&apos;organizzazione</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/elements/1.1/Location -->


    <owl:Class rdf:about="http://purl.org/dc/elements/1.1/Location">
        <owl:equivalentClass rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Location"/>
    </owl:Class>
    


    <!-- http://schema.org/AdministrativeArea -->


    <owl:Class rdf:about="http://schema.org/AdministrativeArea">
        <rdfs:subClassOf rdf:resource="http://schema.org/Place"/>
    </owl:Class>
    


    <!-- http://schema.org/Country -->


    <owl:Class rdf:about="http://schema.org/Country">
        <owl:equivalentClass rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Country"/>
        <rdfs:subClassOf rdf:resource="http://schema.org/AdministrativeArea"/>
    </owl:Class>
    


    <!-- http://schema.org/Organization -->


    <owl:Class rdf:about="http://schema.org/Organization">
        <owl:equivalentClass rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Organization"/>
    </owl:Class>
    


    <!-- http://schema.org/Place -->


    <owl:Class rdf:about="http://schema.org/Place">
        <owl:equivalentClass rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Location"/>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Organization -->


    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Organization">
        <owl:equivalentClass rdf:resource="https://acontenti.github.io/progetto-modsem/survival.ttl#Organization"/>
    </owl:Class>
    


    <!-- http://xmlns.com/foaf/0.1/Organization -->


