@prefix : <https://acontenti.github.io/progetto-modsem/survival.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://acontenti.github.io/progetto-modsem/survival.owl> .

<https://acontenti.github.io/progetto-modsem/survival.owl> rdf:type owl:Ontology ;
                                                            <http://purl.org/dc/elements/1.1/creator> "Alessandro Contenti" ;
                                                            <http://purl.org/dc/elements/1.1/date> "2022-02-04" ;
                                                            <http://purl.org/dc/elements/1.1/description> "An ontology to describe the operations of the Survival International NGO"@en ;
                                                            <http://purl.org/dc/elements/1.1/title> "Survival International Ontology"@en ;
                                                            <http://purl.org/vocab/vannpreferredNamespacePrefix> "survival" ;
                                                            <http://purl.org/vocab/vannpreferredNamespaceUri> "https://acontenti.github.io/progetto-modsem/survival.owl#" ;
                                                            rdfs:label "Survival" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vannpreferredNamespacePrefix
<http://purl.org/vocab/vannpreferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vannpreferredNamespaceUri
<http://purl.org/vocab/vannpreferredNamespaceUri> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  https://acontenti.github.io/progetto-modsem/survival.owl#emailAddress
:emailAddress rdf:type rdfs:Datatype ;
              rdfs:comment "A datatype for an e-mail address"@en ;
              rdfs:label "E-mail"@en ,
                         "E-mail"@it ;
              owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                    owl:onDatatype xsd:string ;
                                    owl:withRestrictions ( [ xsd:pattern "[^@]+@[^\\.]+\\..+"
                                                           ]
                                                         )
                                  ] .


#################################################################
#    Object Properties
#################################################################

###  https://acontenti.github.io/progetto-modsem/survival.owl#addressedBy
:addressedBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :addresses ;
             rdfs:domain :Threat ;
             rdfs:range :Campaign ;
             rdfs:comment "Links a threat to a campaign addressing it"@en ;
             rdfs:label "Addressed by"@en ,
                        "Affrontata da"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#addresses
:addresses rdf:type owl:ObjectProperty ;
           rdfs:domain :Campaign ;
           rdfs:range :Threat ;
           rdfs:comment "Links a campaign to a threat that it addresses"@en ;
           rdfs:label "Addresses"@en ,
                      "Affronta"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#branchOf
:branchOf rdf:type owl:ObjectProperty ;
          owl:inverseOf :hasBranch ;
          rdf:type owl:FunctionalProperty ,
                   owl:AsymmetricProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:domain :BranchOrganization ;
          rdfs:range :Organization ;
          rdfs:comment "Links a branch organization to its head organization"@en ;
          rdfs:label "Branch of"@en ,
                     "Ufficio di"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#cohabitsWith
:cohabitsWith rdf:type owl:ObjectProperty ,
                       owl:SymmetricProperty ;
              rdfs:domain :Tribe ;
              rdfs:range :Tribe ;
              owl:propertyChainAxiom ( :livesIn
                                       [ owl:inverseOf :livesIn
                                       ]
                                     ) ;
              rdfs:comment "Links a tribe to another tribe it shares a habitat with"@en ;
              rdfs:label "Coabita con"@it ,
                         "Cohabits with"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#explicitlyHelps
:explicitlyHelps rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :helps ;
                 rdfs:domain :LocalCampaign ;
                 rdfs:range :Tribe ;
                 rdfs:comment "Links a campaign to a tribe it explicitly helps"@en ;
                 rdfs:label "Aiuta esplicitamente"@it ,
                            "Explicitly helps"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#hasBranch
:hasBranch rdf:type owl:ObjectProperty ,
                    owl:InverseFunctionalProperty ,
                    owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:domain :Organization ;
           rdfs:range :BranchOrganization ;
           rdfs:comment "Links an organization to one of its branches"@en ;
           rdfs:label "Ha ufficio"@it ,
                      "Has branch"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#helpedBy
:helpedBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :helps ;
          rdfs:domain :Tribe ;
          rdfs:range :Campaign ;
          rdfs:comment "Links a tribe to a campaign it is helped by"@en ;
          rdfs:label "Aiutata da"@it ,
                     "Helped by"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#helps
:helps rdf:type owl:ObjectProperty ;
       rdfs:domain :Campaign ;
       rdfs:range :Tribe ;
       owl:propertyChainAxiom ( :addresses
                                :threatens
                              ) ;
       rdfs:comment "Links a campaign to a tribe it implicitly helps"@en ;
       rdfs:label "Aiuta"@it ,
                  "Helps"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#hosts
:hosts rdf:type owl:ObjectProperty ;
       owl:inverseOf :livesIn ;
       rdfs:domain :Habitat ;
       rdfs:range :Tribe ;
       rdfs:comment "Links a habitat to a tribe it hosts"@en ;
       rdfs:label "Hosts"@en ,
                  "Ospita"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#leads
:leads rdf:type owl:ObjectProperty ;
       owl:inverseOf :ledBy ;
       rdfs:domain :Organization ;
       rdfs:range :Campaign ;
       rdfs:comment "Links an organization to a campaign it conducts"@en ;
       rdfs:label "Conduce"@it ,
                  "Leads"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#ledBy
:ledBy rdf:type owl:ObjectProperty ;
       rdfs:domain :Campaign ;
       rdfs:range :Organization ;
       rdfs:comment "Links a campaign to an organization it is conducted by"@en ;
       rdfs:label "Condotta da"@it ,
                  "Led by"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#livesIn
:livesIn rdf:type owl:ObjectProperty ;
         rdfs:domain :Tribe ;
         rdfs:range :Habitat ;
         rdfs:comment "Links a tribe to a habitat it lives in"@en ;
         rdfs:label "Abita in"@it ,
                    "Lives in"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#locatedIn
:locatedIn rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Country
                                       :Habitat
                                       :Organization
                                     )
                       ] ;
           rdfs:range :Location ;
           rdfs:comment "Links a country or a habitat or an organization to a location it is located in"@en ;
           rdfs:label "Located in"@en ,
                      "Situato in"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#threatenedBy
:threatenedBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :threatens ;
              rdfs:domain :Tribe ;
              rdfs:range :Threat ;
              rdfs:comment "Links a tribe to a threat it is endangered by"@en ;
              rdfs:label "Minacciata da"@it ,
                         "Threatened by"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#threatens
:threatens rdf:type owl:ObjectProperty ;
           rdfs:domain :Threat ;
           rdfs:range :Tribe ;
           rdfs:comment "Links a threat to a tribe it endangers"@en ;
           rdfs:label "Minaccia"@it ,
                      "Threatens"@en .


#################################################################
#    Data properties
#################################################################

###  https://acontenti.github.io/progetto-modsem/survival.owl#campaignPage
:campaignPage rdf:type owl:DatatypeProperty ;
              rdfs:domain :Campaign ;
              rdfs:range xsd:anyURI ;
              rdfs:comment "URL of the web page of the campaign"@en ;
              rdfs:label "Campaign web page"@en ,
                         "Pagina web della campagna"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#contacted
:contacted rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Tribe ;
           rdfs:range xsd:boolean ;
           rdfs:comment "Whether a tribe has been contacted or not: defines its uncontacted status"@en ;
           rdfs:label "Contacted tribe"@en ,
                      "Tribù contattata"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#email
:email rdf:type owl:DatatypeProperty ;
       rdfs:domain :Organization ;
       rdfs:range :emailAddress ;
       rdfs:comment "An organization e-mail address"@en ;
       rdfs:label "Indirizzo e-mail dell'organizzazione"@it ,
                  "Organization e-mail address"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#habitatType
:habitatType rdf:type owl:DatatypeProperty ;
             rdfs:domain :Habitat ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:oneOf [ rdf:type rdf:List ;
                                      rdf:first "desert" ;
                                      rdf:rest [ rdf:type rdf:List ;
                                                 rdf:first "forest" ;
                                                 rdf:rest [ rdf:type rdf:List ;
                                                            rdf:first "island" ;
                                                            rdf:rest rdf:nil
                                                          ]
                                               ]
                                    ]
                        ] ;
             rdfs:comment "Defines the type of an habitat, can be either desert, forest or island. Can be used twice to define an intersection, like: forest + island = forested island"@en ;
             rdfs:label "Habitat type"@en ,
                        "Tipo di habitat"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:domain :Tribe ;
      rdfs:range xsd:string ;
      rdfs:comment "The official name of a tribe or indigenous people"@en ;
      rdfs:label "Nome della tribù"@it ,
                 "Tribe name"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#website
:website rdf:type owl:DatatypeProperty ;
         rdfs:domain :Organization ;
         rdfs:range xsd:anyURI ;
         rdfs:comment "URL of the website of an organization"@en ;
         rdfs:label "Organization website"@en ,
                    "Sito web dell'organizzazione"@it .


#################################################################
#    Classes
#################################################################

###  http://purl.org/dc/elements/1.1/Location
<http://purl.org/dc/elements/1.1/Location> rdf:type owl:Class ;
                                           owl:equivalentClass :Location .


###  http://schema.org/AdministrativeArea
<http://schema.org/AdministrativeArea> rdf:type owl:Class ;
                                       rdfs:subClassOf <http://schema.org/Place> .


###  http://schema.org/Country
<http://schema.org/Country> rdf:type owl:Class ;
                            owl:equivalentClass :Country ;
                            rdfs:subClassOf <http://schema.org/AdministrativeArea> .


###  http://schema.org/Organization
<http://schema.org/Organization> rdf:type owl:Class ;
                                 owl:equivalentClass :Organization .


###  http://schema.org/Place
<http://schema.org/Place> rdf:type owl:Class ;
                          owl:equivalentClass :Location .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Organization
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Organization> rdf:type owl:Class ;
                                                                     owl:equivalentClass :Organization .


###  http://xmlns.com/foaf/0.1/Organization
<http://xmlns.com/foaf/0.1/Organization> rdf:type owl:Class ;
                                         owl:equivalentClass :Organization .


###  https://acontenti.github.io/progetto-modsem/survival.owl#BranchOrganization
:BranchOrganization rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :branchOf ;
                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass :Organization
                                        ] ;
                    rdfs:subClassOf :Organization ;
                    rdfs:comment "A local office or branch of an organization. Can be an indipendent legal entity or not."@en ;
                    rdfs:label "Branch office"@en ,
                               "Ufficio distaccato"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Campaign
:Campaign rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :addresses ;
                            owl:someValuesFrom :Threat
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :campaignPage ;
                            owl:someValuesFrom xsd:anyURI
                          ] ;
          rdfs:comment "A generic project intended to mobilize public support and use democratic tools, such as lobbying, addressing some threats, in order to help endangered tribes"@en ;
          rdfs:label "Campagna"@it ,
                     "Campaign"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Continent
:Continent rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( :Africa
                                             :Antarctica
                                             :Asia
                                             :Europe
                                             :NorthAmerica
                                             :Oceania
                                             :SouthAmerica
                                           )
                               ] ;
           rdfs:subClassOf :Location ;
           rdfs:comment "One of the seven geographical regions that make up the global landmass"@en ;
           rdfs:label "Continent"@en ,
                      "Continente"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Country
:Country rdf:type owl:Class ;
         rdfs:subClassOf :Location ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :locatedIn ;
                           owl:someValuesFrom :Continent
                         ] ;
         rdfs:comment "A distinct territorial body or political entity"@en ;
         rdfs:label "Country"@en ,
                    "Paese"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Desert
:Desert rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :habitatType ;
                              owl:hasValue "desert"
                            ] ;
        rdfs:subClassOf :Habitat ;
        rdfs:comment "A desert is a arid or semi-arid area of landscape"@en ;
        rdfs:label "Desert"@en ,
                   "Deserto"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#DesertIsland
:DesertIsland rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :Desert
                                                         :Island
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:comment "An island mostly covered by a desert area"@en ;
              rdfs:label "Desert island"@en ,
                         "Isola desertica"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#DesertTribe
:DesertTribe rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :livesIn ;
                                   owl:someValuesFrom :Desert
                                 ] ;
             rdfs:subClassOf :Tribe ;
             rdfs:comment "Tribe living in a desert"@en ;
             rdfs:label "Desert tribe"@en ,
                        "Tribù del deserto"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Forest
:Forest rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :habitatType ;
                              owl:hasValue "forest"
                            ] ;
        rdfs:subClassOf :Habitat ;
        rdfs:comment "A forest is an area of land dominated by trees"@en ;
        rdfs:label "Forest"@en ,
                   "Foresta"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#ForestTribe
:ForestTribe rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :livesIn ;
                                   owl:someValuesFrom :Forest
                                 ] ;
             rdfs:subClassOf :Tribe ;
             rdfs:comment "Tribe living in a forest"@en ;
             rdfs:label "Forest tribe"@en ,
                        "Tribù della foresta"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#ForestedIsland
:ForestedIsland rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Forest
                                                           :Island
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:comment "An island mostly covered by forests"@en ;
                rdfs:label "Forested island"@en ,
                           "Isola boscosa"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Habitat
:Habitat rdf:type owl:Class ;
         rdfs:subClassOf :Location ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :locatedIn ;
                           owl:someValuesFrom :Country
                         ] ;
         rdfs:comment "A geographical region which may span one or more countries, hosting one or more tribes"@en ;
         rdfs:label "Habitat"@en ,
                    "Habitat"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Island
:Island rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :habitatType ;
                              owl:hasValue "island"
                            ] ;
        rdfs:subClassOf :Habitat ;
        rdfs:comment "An island is a portion of land entirely surrounded by water"@en ;
        rdfs:label "Island"@en ,
                   "Isola"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#IslanderTribe
:IslanderTribe rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :livesIn ;
                                     owl:someValuesFrom :Island
                                   ] ;
               rdfs:subClassOf :Tribe ;
               rdfs:comment "Tribe living on one or more islands"@en ;
               rdfs:label "Islander tribe"@en ,
                          "Tribù isolana"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#LocalCampaign
:LocalCampaign rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :explicitlyHelps ;
                                     owl:someValuesFrom :Tribe
                                   ] ;
               rdfs:subClassOf :Campaign ;
               rdfs:comment "A campaign directly helping one or more specific tribes"@en ;
               rdfs:label "Campagna locale"@it ,
                          "Local campaign"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Location
:Location rdf:type owl:Class ;
          rdfs:comment "A generic location"@en ;
          rdfs:label "Location"@en ,
                     "Luogo"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :locatedIn ;
                                owl:someValuesFrom :Country
                              ] ;
              rdfs:comment "An organization is an entity, such as a company, an institution, or an association, comprising one or more people and having a particular purpose"@en ;
              rdfs:label "Organization"@en ,
                         "Organizzazione"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#SharedCampaign
:SharedCampaign rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :ledBy ;
                                      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                      owl:onClass :Organization
                                    ] ;
                rdfs:subClassOf :Campaign ;
                rdfs:comment "A campaign led by more than one organization"@en ;
                rdfs:label "Campagna condivisa"@it ,
                           "Shared campaign"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Threat
:Threat rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :threatens ;
                          owl:someValuesFrom :Tribe
                        ] ;
        rdfs:comment "A generic threat which may endanger one or more tribe or a tribe-specific threat"@en ;
        rdfs:label "Minaccia"@it ,
                   "Threat"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Tribe
:Tribe rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :livesIn ;
                         owl:someValuesFrom :Habitat
                       ] ;
       rdfs:comment "Endangered tribes and indigenous peoples"@en ;
       rdfs:label "Tribe"@en ,
                  "Tribù"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#UncontactedTribe
:UncontactedTribe rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty :contacted ;
                                        owl:hasValue "false"^^xsd:boolean
                                      ] ;
                  rdfs:subClassOf :Tribe ;
                  rdfs:comment "Tribe that has never, or nearly never, been contacted by the \"civilized\" world, if not for some forced contacts, which have almost always been rejected. Little or nothing is known about these tribes, sometimes just their name or not event that."@en ;
                  rdfs:label "Tribù incontattata"@it ,
                             "Uncontacted tribe"@en .


#################################################################
#    Individuals
#################################################################

###  https://acontenti.github.io/progetto-modsem/survival.owl#APIB
:APIB rdf:type owl:NamedIndividual ,
               :Organization ;
      :locatedIn :Brazil ;
      :email "apib.se@apiboficial.org" ;
      :website "https://apiboficial.org/"^^xsd:anyURI ;
      rdfs:comment "The Articulation of Indigenous Peoples of Brazil was created by the indigenous movement at Camp Terra Livre (ATL) 2005. APIB is an instance of national reference of the indigenous movement in Brazil, created from the bottom up."@en ;
      rdfs:label "Articolazione dei Popoli Indigeni del Brasile"@it ,
                 "Articulation of Indigenous Peoples of Brazil"@en .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Africa
:Africa rdf:type owl:NamedIndividual ,
                 :Continent ;
        rdfs:comment "The African continent"@en ;
        rdfs:label "Africa"@en ,
                   "Africa"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Antarctica
:Antarctica rdf:type owl:NamedIndividual ,
                     :Continent ;
            rdfs:comment "The Antarctic continent"@en ;
            rdfs:label "Antarctica"@en ,
                       "Antartide"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Asia
:Asia rdf:type owl:NamedIndividual ,
               :Continent ;
      rdfs:comment "The continent of Asia"@en ;
      rdfs:label "Asia"@en ,
                 "Asia"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Bolivia
:Bolivia rdf:type owl:NamedIndividual ,
                  :Country ;
         :locatedIn :SouthAmerica ;
         rdfs:comment "The Plurinational State of Bolivia"@en ;
         rdfs:label "Bolivia"@en ,
                    "Bolivia"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Botswana
:Botswana rdf:type owl:NamedIndividual ,
                   :Country ;
          :locatedIn :Africa ;
          rdfs:comment "The Republic of Botswana"@en ;
          rdfs:label "Botswana"@en ,
                     "Botswana"@it .


###  https://acontenti.github.io/progetto-modsem/survival.owl#Brazil
:Brazil rdf:type owl:NamedIndividual ,
                 :Country ;
        :locatedIn :SouthAmerica ;
