@prefix : <http://www.semanticweb.org/acontenti/ontologies/survival#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix schema: <http://schema.org/> .
@prefix survival: <http://www.semanticweb.org/acontenti/ontologies/survival#> .
@base <http://www.semanticweb.org/acontenti/ontologies/survival> .

<http://www.semanticweb.org/acontenti/ontologies/survival> rdf:type owl:Ontology ;
                                                            dc:creator "Alessandro Contenti" ;
                                                            dc:date "2022-02-04" ;
                                                            dc:description "An ontology to describe the operations of the Survival International NGO"@en ;
                                                            dc:title "Survival International Ontology"@en ;
                                                            vann:preferredNamespacePrefix "survival" ;
                                                            vann:preferredNamespaceUri "http://www.semanticweb.org/acontenti/ontologies/survival#" ;
                                                            rdfs:label "Survival" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.semanticweb.org/acontenti/ontologies/survival#emailAddress
survival:emailAddress rdf:type rdfs:Datatype ;
                      rdfs:comment "A datatype for an e-mail address"@en ;
                      rdfs:label "E-mail"@en ,
                                 "E-mail"@it ;
                      owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                            owl:onDatatype xsd:string ;
                                            owl:withRestrictions ( [ xsd:pattern "[^@]+@[^\\.]+\\..+"
                                                                   ]
                                                                 )
                                          ] .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/acontenti/ontologies/survival#addressedBy
survival:addressedBy rdf:type owl:ObjectProperty ;
                     owl:inverseOf survival:addresses ;
                     rdfs:domain survival:Threat ;
                     rdfs:range survival:Campaign ;
                     rdfs:comment "Links a threat to a campaign addressing it"@en ;
                     rdfs:label "Addressed by"@en ,
                                "Affrontata da"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#addresses
survival:addresses rdf:type owl:ObjectProperty ;
                   rdfs:domain survival:Campaign ;
                   rdfs:range survival:Threat ;
                   rdfs:comment "Links a campaign to a threat that it addresses"@en ;
                   rdfs:label "Addresses"@en ,
                              "Affronta"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#branchOf
survival:branchOf rdf:type owl:ObjectProperty ;
                  owl:inverseOf survival:hasBranch ;
                  rdf:type owl:FunctionalProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain survival:BranchOrganization ;
                  rdfs:range survival:Organization ;
                  rdfs:comment "Links a branch organization to its head organization"@en ;
                  rdfs:label "Branch of"@en ,
                             "Ufficio di"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#cohabitsWith
survival:cohabitsWith rdf:type owl:ObjectProperty ,
                               owl:SymmetricProperty ;
                      rdfs:domain survival:Tribe ;
                      rdfs:range survival:Tribe ;
                      owl:propertyChainAxiom ( survival:livesIn
                                               [ owl:inverseOf survival:livesIn
                                               ]
                                             ) ;
                      rdfs:comment "Links a tribe to another tribe it shares a habitat with"@en ;
                      rdfs:label "Coabita con"@it ,
                                 "Cohabits with"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#explicitlyHelps
survival:explicitlyHelps rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf survival:helps ;
                         rdfs:domain survival:LocalCampaign ;
                         rdfs:range survival:Tribe ;
                         rdfs:comment "Links a campaign to a tribe it explicitly helps"@en ;
                         rdfs:label "Aiuta esplicitamente"@it ,
                                    "Explicitly helps"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#hasBranch
survival:hasBranch rdf:type owl:ObjectProperty ,
                            owl:InverseFunctionalProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain survival:Organization ;
                   rdfs:range survival:BranchOrganization ;
                   rdfs:comment "Links an organization to one of its branches"@en ;
                   rdfs:label "Ha ufficio"@it ,
                              "Has branch"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#helpedBy
survival:helpedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf survival:helps ;
                  rdfs:domain survival:Tribe ;
                  rdfs:range survival:Campaign ;
                  rdfs:comment "Links a tribe to a campaign it is helped by"@en ;
                  rdfs:label "Aiutata da"@it ,
                             "Helped by"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#helps
survival:helps rdf:type owl:ObjectProperty ;
               rdfs:domain survival:Campaign ;
               rdfs:range survival:Tribe ;
               owl:propertyChainAxiom ( survival:addresses
                                        survival:threatens
                                      ) ;
               rdfs:comment "Links a campaign to a tribe it implicitly helps"@en ;
               rdfs:label "Aiuta"@it ,
                          "Helps"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#hosts
survival:hosts rdf:type owl:ObjectProperty ;
               owl:inverseOf survival:livesIn ;
               rdfs:domain survival:Habitat ;
               rdfs:range survival:Tribe ;
               rdfs:comment "Links a habitat to a tribe it hosts"@en ;
               rdfs:label "Hosts"@en ,
                          "Ospita"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#leads
survival:leads rdf:type owl:ObjectProperty ;
               owl:inverseOf survival:ledBy ;
               rdfs:domain survival:Organization ;
               rdfs:range survival:Campaign ;
               rdfs:comment "Links an organization to a campaign it conducts"@en ;
               rdfs:label "Conduce"@it ,
                          "Leads"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#ledBy
survival:ledBy rdf:type owl:ObjectProperty ;
               rdfs:domain survival:Campaign ;
               rdfs:range survival:Organization ;
               rdfs:comment "Links a campaign to an organization it is conducted by"@en ;
               rdfs:label "Condotta da"@it ,
                          "Led by"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#livesIn
survival:livesIn rdf:type owl:ObjectProperty ;
                 rdfs:domain survival:Tribe ;
                 rdfs:range survival:Habitat ;
                 rdfs:comment "Links a tribe to a habitat it lives in"@en ;
                 rdfs:label "Abita in"@it ,
                            "Lives in"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#locatedIn
survival:locatedIn rdf:type owl:ObjectProperty ,
                            owl:TransitiveProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( survival:Country
                                               survival:Habitat
                                               survival:Organization
                                             )
                               ] ;
                   rdfs:range survival:Location ;
                   rdfs:comment "Links a country or a habitat or an organization to a location it is located in"@en ;
                   rdfs:label "Located in"@en ,
                              "Situato in"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#threatenedBy
survival:threatenedBy rdf:type owl:ObjectProperty ;
                      owl:inverseOf survival:threatens ;
                      rdfs:domain survival:Tribe ;
                      rdfs:range survival:Threat ;
                      rdfs:comment "Links a tribe to a threat it is endangered by"@en ;
                      rdfs:label "Minacciata da"@it ,
                                 "Threatened by"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#threatens
survival:threatens rdf:type owl:ObjectProperty ;
                   rdfs:domain survival:Threat ;
                   rdfs:range survival:Tribe ;
                   rdfs:comment "Links a threat to a tribe it endangers"@en ;
                   rdfs:label "Minaccia"@it ,
                              "Threatens"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/acontenti/ontologies/survival#campaignPage
survival:campaignPage rdf:type owl:DatatypeProperty ;
                      rdfs:domain survival:Campaign ;
                      rdfs:range xsd:anyURI ;
                      rdfs:comment "URL of the web page of the campaign"@en ;
                      rdfs:label "Campaign web page"@en ,
                                 "Pagina web della campagna"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#contacted
survival:contacted rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain survival:Tribe ;
                   rdfs:range xsd:boolean ;
                   rdfs:comment "Whether a tribe has been contacted or not: defines its uncontacted status"@en ;
                   rdfs:label "Contacted tribe"@en ,
                              "Tribù contattata"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#email
survival:email rdf:type owl:DatatypeProperty ;
               rdfs:domain survival:Organization ;
               rdfs:range survival:emailAddress ;
               rdfs:comment "An organization e-mail address"@en ;
               rdfs:label "Indirizzo e-mail dell'organizzazione"@it ,
                          "Organization e-mail address"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#habitatType
survival:habitatType rdf:type owl:DatatypeProperty ;
                     rdfs:domain survival:Habitat ;
                     rdfs:range [ rdf:type rdfs:Datatype ;
                                  owl:oneOf [ rdf:type rdf:List ;
                                              rdf:first "desert" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "forest" ;
                                                         rdf:rest [ rdf:type rdf:List ;
                                                                    rdf:first "island" ;
                                                                    rdf:rest rdf:nil
                                                                  ]
                                                       ]
                                            ]
                                ] ;
                     rdfs:comment "Defines the type of an habitat, can be either desert, forest or island. Can be used twice to define an intersection, like: forest + island = forested island"@en ;
                     rdfs:label "Habitat type"@en ,
                                "Tipo di habitat"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#name
survival:name rdf:type owl:DatatypeProperty ;
              rdfs:domain survival:Tribe ;
              rdfs:range xsd:string ;
              rdfs:comment "The official name of a tribe or indigenous people"@en ;
              rdfs:label "Nome della tribù"@it ,
                         "Tribe name"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#website
survival:website rdf:type owl:DatatypeProperty ;
                 rdfs:domain survival:Organization ;
                 rdfs:range xsd:anyURI ;
                 rdfs:comment "URL of the website of an organization"@en ;
                 rdfs:label "Organization website"@en ,
                            "Sito web dell'organizzazione"@it .


#################################################################
#    Classes
#################################################################

###  http://purl.org/dc/elements/1.1/Location
dc:Location rdf:type owl:Class ;
            owl:equivalentClass survival:Location .


###  http://schema.org/AdministrativeArea
schema:AdministrativeArea rdf:type owl:Class ;
                          rdfs:subClassOf schema:Place .


###  http://schema.org/Country
schema:Country rdf:type owl:Class ;
               owl:equivalentClass survival:Country ;
               rdfs:subClassOf schema:AdministrativeArea .


###  http://schema.org/Organization
schema:Organization rdf:type owl:Class ;
                    owl:equivalentClass survival:Organization .


###  http://schema.org/Place
schema:Place rdf:type owl:Class ;
             owl:equivalentClass survival:Location .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Organization
dul:Organization rdf:type owl:Class ;
                 owl:equivalentClass survival:Organization .


###  http://www.semanticweb.org/acontenti/ontologies/survival#BranchOrganization
survival:BranchOrganization rdf:type owl:Class ;
                            owl:equivalentClass [ rdf:type owl:Restriction ;
                                                  owl:onProperty survival:branchOf ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onClass survival:Organization
                                                ] ;
                            rdfs:subClassOf survival:Organization ;
                            rdfs:comment "A local office or branch of an organization. Can be an indipendent legal entity or not."@en ;
                            rdfs:label "Branch office"@en ,
                                       "Ufficio distaccato"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Campaign
survival:Campaign rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty survival:addresses ;
                                    owl:someValuesFrom survival:Threat
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty survival:campaignPage ;
                                    owl:someValuesFrom xsd:anyURI
                                  ] ;
                  rdfs:comment "A generic project intended to mobilize public support and use democratic tools, such as lobbying, addressing some threats, in order to help endangered tribes"@en ;
                  rdfs:label "Campagna"@it ,
                             "Campaign"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Continent
survival:Continent rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:oneOf ( survival:Africa
                                                     survival:Antarctica
                                                     survival:Asia
                                                     survival:Europe
                                                     survival:NorthAmerica
                                                     survival:Oceania
                                                     survival:SouthAmerica
                                                   )
                                       ] ;
                   rdfs:subClassOf survival:Location ;
                   rdfs:comment "One of the seven geographical regions that make up the global landmass"@en ;
                   rdfs:label "Continent"@en ,
                              "Continente"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Country
survival:Country rdf:type owl:Class ;
                 rdfs:subClassOf survival:Location ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty survival:locatedIn ;
                                   owl:someValuesFrom survival:Continent
                                 ] ;
                 rdfs:comment "A distinct territorial body or political entity"@en ;
                 rdfs:label "Country"@en ,
                            "Paese"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Desert
survival:Desert rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty survival:habitatType ;
                                      owl:hasValue "desert"
                                    ] ;
                rdfs:subClassOf survival:Habitat ;
                rdfs:comment "A desert is a arid or semi-arid area of landscape"@en ;
                rdfs:label "Desert"@en ,
                           "Deserto"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#DesertIsland
survival:DesertIsland rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( survival:Desert
                                                                 survival:Island
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:comment "An island mostly covered by a desert area"@en ;
                      rdfs:label "Desert island"@en ,
                                 "Isola desertica"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#DesertTribe
survival:DesertTribe rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty survival:livesIn ;
                                           owl:someValuesFrom survival:Desert
                                         ] ;
                     rdfs:subClassOf survival:Tribe ;
                     rdfs:comment "Tribe living in a desert"@en ;
                     rdfs:label "Desert tribe"@en ,
                                "Tribù del deserto"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Forest
survival:Forest rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty survival:habitatType ;
                                      owl:hasValue "forest"
                                    ] ;
                rdfs:subClassOf survival:Habitat ;
                rdfs:comment "A forest is an area of land dominated by trees"@en ;
                rdfs:label "Forest"@en ,
                           "Foresta"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#ForestTribe
survival:ForestTribe rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty survival:livesIn ;
                                           owl:someValuesFrom survival:Forest
                                         ] ;
                     rdfs:subClassOf survival:Tribe ;
                     rdfs:comment "Tribe living in a forest"@en ;
                     rdfs:label "Forest tribe"@en ,
                                "Tribù della foresta"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#ForestedIsland
survival:ForestedIsland rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( survival:Forest
                                                                   survival:Island
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:comment "An island mostly covered by forests"@en ;
                        rdfs:label "Forested island"@en ,
                                   "Isola boscosa"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Habitat
survival:Habitat rdf:type owl:Class ;
                 rdfs:subClassOf survival:Location ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty survival:locatedIn ;
                                   owl:someValuesFrom survival:Country
                                 ] ;
                 rdfs:comment "A geographical region which may span one or more countries, hosting one or more tribes"@en ;
                 rdfs:label "Habitat"@en ,
                            "Habitat"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Island
survival:Island rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty survival:habitatType ;
                                      owl:hasValue "island"
                                    ] ;
                rdfs:subClassOf survival:Habitat ;
                rdfs:comment "An island is a portion of land entirely surrounded by water"@en ;
                rdfs:label "Island"@en ,
                           "Isola"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#IslanderTribe
survival:IslanderTribe rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty survival:livesIn ;
                                             owl:someValuesFrom survival:Island
                                           ] ;
                       rdfs:subClassOf survival:Tribe ;
                       rdfs:comment "Tribe living on one or more islands"@en ;
                       rdfs:label "Islander tribe"@en ,
                                  "Tribù isolana"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#LocalCampaign
survival:LocalCampaign rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty survival:explicitlyHelps ;
                                             owl:someValuesFrom survival:Tribe
                                           ] ;
                       rdfs:subClassOf survival:Campaign ;
                       rdfs:comment "A campaign directly helping one or more specific tribes"@en ;
                       rdfs:label "Campagna locale"@it ,
                                  "Local campaign"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Location
survival:Location rdf:type owl:Class ;
                  rdfs:comment "A generic location"@en ;
                  rdfs:label "Location"@en ,
                             "Luogo"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Organization
survival:Organization rdf:type owl:Class ;
                      owl:equivalentClass foaf:Organization ;
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty survival:locatedIn ;
                                        owl:someValuesFrom survival:Country
                                      ] ;
                      rdfs:comment "An organization is an entity, such as a company, an institution, or an association, comprising one or more people and having a particular purpose"@en ;
                      rdfs:label "Organization"@en ,
                                 "Organizzazione"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#SharedCampaign
survival:SharedCampaign rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                              owl:onProperty survival:ledBy ;
                                              owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                              owl:onClass survival:Organization
                                            ] ;
                        rdfs:subClassOf survival:Campaign ;
                        rdfs:comment "A campaign led by more than one organization"@en ;
                        rdfs:label "Campagna condivisa"@it ,
                                   "Shared campaign"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Threat
survival:Threat rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty survival:threatens ;
                                  owl:someValuesFrom survival:Tribe
                                ] ;
                rdfs:comment "A generic threat which may endanger one or more tribe or a tribe-specific threat"@en ;
                rdfs:label "Minaccia"@it ,
                           "Threat"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Tribe
survival:Tribe rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty survival:livesIn ;
                                 owl:someValuesFrom survival:Habitat
                               ] ;
               rdfs:comment "Endangered tribes and indigenous peoples"@en ;
               rdfs:label "Tribe"@en ,
                          "Tribù"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#UncontactedTribe
survival:UncontactedTribe rdf:type owl:Class ;
                          owl:equivalentClass [ rdf:type owl:Restriction ;
                                                owl:onProperty survival:contacted ;
                                                owl:hasValue "false"^^xsd:boolean
                                              ] ;
                          rdfs:subClassOf survival:Tribe ;
                          rdfs:comment "Tribe that has never, or nearly never, been contacted by the \"civilized\" world, if not for some forced contacts, which have almost always been rejected. Little or nothing is known about these tribes, sometimes just their name or not event that."@en ;
                          rdfs:label "Tribù incontattata"@it ,
                                     "Uncontacted tribe"@en .


###  http://xmlns.com/foaf/0.1/Organization
foaf:Organization rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/acontenti/ontologies/survival#APIB
survival:APIB rdf:type owl:NamedIndividual ,
                       survival:Organization ;
              survival:locatedIn survival:Brazil ;
              survival:email "apib.se@apiboficial.org" ;
              survival:website "https://apiboficial.org/"^^xsd:anyURI ;
              rdfs:comment "The Articulation of Indigenous Peoples of Brazil was created by the indigenous movement at Camp Terra Livre (ATL) 2005. APIB is an instance of national reference of the indigenous movement in Brazil, created from the bottom up."@en ;
              rdfs:label "Articolazione dei Popoli Indigeni del Brasile"@it ,
                         "Articulation of Indigenous Peoples of Brazil"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Africa
survival:Africa rdf:type owl:NamedIndividual ,
                         survival:Continent ;
                rdfs:comment "The African continent"@en ;
                rdfs:label "Africa"@en ,
                           "Africa"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Antarctica
survival:Antarctica rdf:type owl:NamedIndividual ,
                             survival:Continent ;
                    rdfs:comment "The Antarctic continent"@en ;
                    rdfs:label "Antarctica"@en ,
                               "Antartide"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Asia
survival:Asia rdf:type owl:NamedIndividual ,
                       survival:Continent ;
              rdfs:comment "The continent of Asia"@en ;
              rdfs:label "Asia"@en ,
                         "Asia"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Bolivia
survival:Bolivia rdf:type owl:NamedIndividual ,
                          survival:Country ;
                 survival:locatedIn survival:SouthAmerica ;
                 rdfs:comment "The Plurinational State of Bolivia"@en ;
                 rdfs:label "Bolivia"@en ,
                            "Bolivia"@it .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Botswana
survival:Botswana rdf:type owl:NamedIndividual ,
                           survival:Country ;
                  survival:locatedIn survival:Africa ;
                  rdfs:comment "The Republic of Botswana"@en ;
