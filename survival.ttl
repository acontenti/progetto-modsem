@prefix : <http://www.semanticweb.org/acontenti/ontologies/survival#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann> .
@prefix survival: <http://www.semanticweb.org/acontenti/ontologies/survival#> .
@base <http://www.semanticweb.org/acontenti/ontologies/survival> .

<http://www.semanticweb.org/acontenti/ontologies/survival> rdf:type owl:Ontology ;
                                                            <http://purl.org/dc/elements/1.1/creator> "Alessandro Contenti" ;
                                                            <http://purl.org/dc/elements/1.1/date> "2022-01-19" ;
                                                            <http://purl.org/dc/elements/1.1/description> "An ontology to describe the operations of the NGO \"Survival International\"" ;
                                                            <http://purl.org/dc/elements/1.1/title> "Survival International Ontology" ;
                                                            <http://purl.org/vocab/vann/preferredNamespacePrefix> "survival" ;
                                                            <http://purl.org/vocab/vann/preferredNamespaceUri> "http://www.semanticweb.org/acontenti/ontologies/survival#" ;
                                                            rdfs:label "Survival" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.semanticweb.org/acontenti/ontologies/survival#emailAddress
survival:emailAddress rdf:type rdfs:Datatype ;
                      owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                            owl:onDatatype xsd:string ;
                                            owl:withRestrictions ( [ xsd:pattern "[^@]+@[^\\.]+\\..+"
                                                                   ]
                                                                 )
                                          ] .


###  http://www.semanticweb.org/acontenti/ontologies/survival#websiteURL
survival:websiteURL rdf:type rdfs:Datatype ;
                    owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                          owl:onDatatype xsd:string ;
                                          owl:withRestrictions ( [ xsd:pattern "[hH][tT][tT][pP][sS]?:\\/\\/.+"
                                                                 ]
                                                               )
                                        ] .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/acontenti/ontologies/survival#addressedBy
survival:addressedBy rdf:type owl:ObjectProperty ;
                     owl:inverseOf survival:addresses ;
                     rdfs:domain survival:Threat ;
                     rdfs:range survival:Campaign .


###  http://www.semanticweb.org/acontenti/ontologies/survival#addresses
survival:addresses rdf:type owl:ObjectProperty ;
                   rdfs:domain survival:Campaign ;
                   rdfs:range survival:Threat .


###  http://www.semanticweb.org/acontenti/ontologies/survival#branchOf
survival:branchOf rdf:type owl:ObjectProperty ;
                  owl:inverseOf survival:hasBranch ;
                  rdf:type owl:FunctionalProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain survival:BranchOrganization ;
                  rdfs:range survival:Organization .


###  http://www.semanticweb.org/acontenti/ontologies/survival#cohabits
survival:cohabits rdf:type owl:ObjectProperty ,
                           owl:SymmetricProperty ;
                  rdfs:domain survival:Tribe ;
                  rdfs:range survival:Tribe ;
                  owl:propertyChainAxiom ( survival:livesIn
                                           [ owl:inverseOf survival:livesIn
                                           ]
                                         ) .


###  http://www.semanticweb.org/acontenti/ontologies/survival#directlyHelps
survival:directlyHelps rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf survival:helps ;
                       rdfs:domain survival:LocalCampaign ;
                       rdfs:range survival:Tribe .


###  http://www.semanticweb.org/acontenti/ontologies/survival#hasBranch
survival:hasBranch rdf:type owl:ObjectProperty ,
                            owl:InverseFunctionalProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain survival:Organization ;
                   rdfs:range survival:BranchOrganization .


###  http://www.semanticweb.org/acontenti/ontologies/survival#helpedBy
survival:helpedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf survival:helps ;
                  rdfs:domain survival:Tribe ;
                  rdfs:range survival:Campaign ;
                  owl:propertyChainAxiom ( survival:threatenedBy
                                           survival:addressedBy
                                         ) .


###  http://www.semanticweb.org/acontenti/ontologies/survival#helps
survival:helps rdf:type owl:ObjectProperty ;
               rdfs:domain survival:Campaign ;
               rdfs:range survival:Tribe .


###  http://www.semanticweb.org/acontenti/ontologies/survival#hosts
survival:hosts rdf:type owl:ObjectProperty ;
               owl:inverseOf survival:livesIn ;
               rdfs:domain survival:Habitat ;
               rdfs:range survival:Tribe .


###  http://www.semanticweb.org/acontenti/ontologies/survival#leads
survival:leads rdf:type owl:ObjectProperty ;
               owl:inverseOf survival:ledBy ;
               rdfs:domain survival:Organization ;
               rdfs:range survival:Campaign .


###  http://www.semanticweb.org/acontenti/ontologies/survival#ledBy
survival:ledBy rdf:type owl:ObjectProperty ;
               rdfs:domain survival:Campaign ;
               rdfs:range survival:Organization .


###  http://www.semanticweb.org/acontenti/ontologies/survival#livesIn
survival:livesIn rdf:type owl:ObjectProperty ;
                 rdfs:domain survival:Tribe ;
                 rdfs:range survival:Habitat .


###  http://www.semanticweb.org/acontenti/ontologies/survival#locatedIn
survival:locatedIn rdf:type owl:ObjectProperty ,
                            owl:TransitiveProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( survival:Country
                                               survival:Habitat
                                               survival:Organization
                                             )
                               ] ;
                   rdfs:range survival:Location .


###  http://www.semanticweb.org/acontenti/ontologies/survival#threatenedBy
survival:threatenedBy rdf:type owl:ObjectProperty ;
                      owl:inverseOf survival:threatens ;
                      rdfs:domain survival:Tribe ;
                      rdfs:range survival:Threat .


###  http://www.semanticweb.org/acontenti/ontologies/survival#threatens
survival:threatens rdf:type owl:ObjectProperty ;
                   rdfs:domain survival:Threat ;
                   rdfs:range survival:Tribe .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/acontenti/ontologies/survival#campaignPage
survival:campaignPage rdf:type owl:DatatypeProperty ;
                      rdfs:domain survival:Campaign ;
                      rdfs:range xsd:anyURI .


###  http://www.semanticweb.org/acontenti/ontologies/survival#contacted
survival:contacted rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain survival:Tribe ;
                   rdfs:range xsd:boolean .


###  http://www.semanticweb.org/acontenti/ontologies/survival#email
survival:email rdf:type owl:DatatypeProperty ;
               rdfs:domain survival:Organization ;
               rdfs:range survival:emailAddress .


###  http://www.semanticweb.org/acontenti/ontologies/survival#habitatType
survival:habitatType rdf:type owl:DatatypeProperty ;
                     rdfs:domain survival:Habitat ;
                     rdfs:range [ rdf:type rdfs:Datatype ;
                                  owl:oneOf [ rdf:type rdf:List ;
                                              rdf:first "desert" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "forest" ;
                                                         rdf:rest [ rdf:type rdf:List ;
                                                                    rdf:first "island" ;
                                                                    rdf:rest rdf:nil
                                                                  ]
                                                       ]
                                            ]
                                ] .


###  http://www.semanticweb.org/acontenti/ontologies/survival#name
survival:name rdf:type owl:DatatypeProperty ;
              rdfs:domain survival:Tribe ;
              rdfs:range xsd:string .


###  http://www.semanticweb.org/acontenti/ontologies/survival#website
survival:website rdf:type owl:DatatypeProperty ;
                 rdfs:domain survival:Organization ;
                 rdfs:range xsd:anyURI .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/acontenti/ontologies/survival#BranchOrganization
survival:BranchOrganization rdf:type owl:Class ;
                            owl:equivalentClass [ rdf:type owl:Restriction ;
                                                  owl:onProperty survival:branchOf ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onClass survival:Organization
                                                ] ;
                            rdfs:subClassOf survival:Organization .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Campaign
survival:Campaign rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty survival:addresses ;
                                    owl:someValuesFrom survival:Threat
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty survival:campaignPage ;
                                    owl:someValuesFrom xsd:anyURI
                                  ] .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Continent
survival:Continent rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:oneOf ( survival:Africa
                                                     survival:Asia
                                                     survival:Europe
                                                     survival:NorthAmerica
                                                     survival:Oceania
                                                     survival:SouthAmerica
                                                   )
                                       ] ;
                   rdfs:subClassOf survival:Location .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Country
survival:Country rdf:type owl:Class ;
                 rdfs:subClassOf survival:Location ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty survival:locatedIn ;
                                   owl:someValuesFrom survival:Continent
                                 ] .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Desert
survival:Desert rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty survival:habitatType ;
                                      owl:hasValue "desert"
                                    ] ;
                rdfs:subClassOf survival:Habitat .


###  http://www.semanticweb.org/acontenti/ontologies/survival#DesertTribe
survival:DesertTribe rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty survival:livesIn ;
                                           owl:someValuesFrom survival:Desert
                                         ] ;
                     rdfs:subClassOf survival:Tribe .


###  http://www.semanticweb.org/acontenti/ontologies/survival#DesertedIsland
survival:DesertedIsland rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( survival:Desert
                                                                   survival:Island
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Forest
survival:Forest rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty survival:habitatType ;
                                      owl:hasValue "forest"
                                    ] ;
                rdfs:subClassOf survival:Habitat .


###  http://www.semanticweb.org/acontenti/ontologies/survival#ForestTribe
survival:ForestTribe rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty survival:livesIn ;
                                           owl:someValuesFrom survival:Forest
                                         ] ;
                     rdfs:subClassOf survival:Tribe .


###  http://www.semanticweb.org/acontenti/ontologies/survival#ForestedIsland
survival:ForestedIsland rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( survival:Forest
                                                                   survival:Island
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Habitat
survival:Habitat rdf:type owl:Class ;
                 rdfs:subClassOf survival:Location ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty survival:locatedIn ;
                                   owl:someValuesFrom survival:Country
                                 ] .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Island
survival:Island rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty survival:habitatType ;
                                      owl:hasValue "island"
                                    ] ;
                rdfs:subClassOf survival:Habitat .


###  http://www.semanticweb.org/acontenti/ontologies/survival#IslanderTribe
survival:IslanderTribe rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty survival:livesIn ;
                                             owl:someValuesFrom survival:Island
                                           ] ;
                       rdfs:subClassOf survival:Tribe .


###  http://www.semanticweb.org/acontenti/ontologies/survival#LocalCampaign
survival:LocalCampaign rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty survival:directlyHelps ;
                                             owl:someValuesFrom survival:Tribe
                                           ] ;
                       rdfs:subClassOf survival:Campaign .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Location
survival:Location rdf:type owl:Class .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Organization
survival:Organization rdf:type owl:Class ;
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty survival:locatedIn ;
                                        owl:someValuesFrom survival:Country
                                      ] .


###  http://www.semanticweb.org/acontenti/ontologies/survival#SharedCampaign
survival:SharedCampaign rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                              owl:onProperty survival:ledBy ;
                                              owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                              owl:onClass survival:Organization
                                            ] ;
                        rdfs:subClassOf survival:Campaign .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Threat
survival:Threat rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty survival:threatens ;
                                  owl:someValuesFrom survival:Tribe
                                ] .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Tribe
survival:Tribe rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty survival:livesIn ;
                                 owl:someValuesFrom survival:Habitat
                               ] .


###  http://www.semanticweb.org/acontenti/ontologies/survival#UncontactedTribe
survival:UncontactedTribe rdf:type owl:Class ;
                          owl:equivalentClass [ rdf:type owl:Restriction ;
                                                owl:onProperty survival:contacted ;
                                                owl:hasValue "false"^^xsd:boolean
                                              ] ;
                          rdfs:subClassOf survival:Tribe .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/acontenti/ontologies/survival#APIB
survival:APIB rdf:type owl:NamedIndividual ,
                       survival:Organization ;
              survival:locatedIn survival:Brazil ;
              rdfs:label "Articulation of Indigenous Peoples of Brazil"@en ,
                         "Articulação dos Povos Indígenas do Brasil"@pt .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Africa
survival:Africa rdf:type owl:NamedIndividual ,
                         survival:Continent .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Asia
survival:Asia rdf:type owl:NamedIndividual ,
                       survival:Continent .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Bolivia
survival:Bolivia rdf:type owl:NamedIndividual ,
                          survival:Country ;
                 survival:locatedIn survival:SouthAmerica .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Botswana
survival:Botswana rdf:type owl:NamedIndividual ,
                           survival:Country ;
                  survival:locatedIn survival:Africa .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Brazil
survival:Brazil rdf:type owl:NamedIndividual ,
                         survival:Country ;
                survival:locatedIn survival:SouthAmerica .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Bushmen
survival:Bushmen rdf:type owl:NamedIndividual ,
                          survival:Tribe ;
                 survival:livesIn survival:CentralKalahariGameReserve ;
                 survival:threatenedBy survival:ColonialConservation ;
                 survival:contacted "true"^^xsd:boolean .


###  http://www.semanticweb.org/acontenti/ontologies/survival#CentralKalahariGameReserve
survival:CentralKalahariGameReserve rdf:type owl:NamedIndividual ,
                                             survival:Habitat ;
                                    survival:locatedIn survival:Botswana ;
                                    survival:habitatType "desert" .


###  http://www.semanticweb.org/acontenti/ontologies/survival#ColonialConservation
survival:ColonialConservation rdf:type owl:NamedIndividual ,
                                       survival:Threat .


###  http://www.semanticweb.org/acontenti/ontologies/survival#DecolonizeConservation
survival:DecolonizeConservation rdf:type owl:NamedIndividual ,
                                         survival:Campaign ;
                                survival:addresses survival:ColonialConservation .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Deforestation
survival:Deforestation rdf:type owl:NamedIndividual ,
                                survival:Threat .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Europe
survival:Europe rdf:type owl:NamedIndividual ,
                         survival:Continent .


###  http://www.semanticweb.org/acontenti/ontologies/survival#ForcedContact
survival:ForcedContact rdf:type owl:NamedIndividual ,
                                survival:Threat .


###  http://www.semanticweb.org/acontenti/ontologies/survival#IllegalMining
survival:IllegalMining rdf:type owl:NamedIndividual ,
                                survival:Threat .


###  http://www.semanticweb.org/acontenti/ontologies/survival#India
survival:India rdf:type owl:NamedIndividual ,
                        survival:Country ;
               survival:locatedIn survival:Asia .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Italy
survival:Italy rdf:type owl:NamedIndividual ,
                        survival:Country ;
               survival:locatedIn survival:Europe .


###  http://www.semanticweb.org/acontenti/ontologies/survival#LandExploitation
survival:LandExploitation rdf:type owl:NamedIndividual ,
                                   survival:Threat .


###  http://www.semanticweb.org/acontenti/ontologies/survival#MashcoPiro
survival:MashcoPiro rdf:type owl:NamedIndividual ,
                             survival:Tribe ;
                    survival:livesIn survival:UncontactedFrontier ;
                    survival:threatenedBy survival:ForcedContact ,
                                          survival:LandExploitation ;
                    survival:contacted "false"^^xsd:boolean ;
                    survival:name "Mashco-Piro"^^xsd:string .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Matses
survival:Matses rdf:type owl:NamedIndividual ,
                         survival:Tribe ;
                survival:livesIn survival:UncontactedFrontier ;
                survival:threatenedBy survival:ForcedContact ,
                                      survival:LandExploitation ;
                survival:contacted "false"^^xsd:boolean ;
                survival:name "Matsés"^^xsd:string .


###  http://www.semanticweb.org/acontenti/ontologies/survival#NorthAmerica
survival:NorthAmerica rdf:type owl:NamedIndividual ,
                               survival:Continent .


###  http://www.semanticweb.org/acontenti/ontologies/survival#NorthSentinel
survival:NorthSentinel rdf:type owl:NamedIndividual ,
                                survival:Habitat ;
                       survival:locatedIn survival:India ;
                       survival:habitatType "forest" ,
                                            "island" .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Oceania
survival:Oceania rdf:type owl:NamedIndividual ,
                          survival:Continent .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Peru
survival:Peru rdf:type owl:NamedIndividual ,
                       survival:Country ;
              survival:locatedIn survival:SouthAmerica .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Sentinelese
survival:Sentinelese rdf:type owl:NamedIndividual ,
                              survival:Tribe ;
                     survival:livesIn survival:NorthSentinel ;
                     survival:threatenedBy survival:ForcedContact ;
                     survival:contacted "false"^^xsd:boolean .


###  http://www.semanticweb.org/acontenti/ontologies/survival#SouthAmerica
survival:SouthAmerica rdf:type owl:NamedIndividual ,
                               survival:Continent .


###  http://www.semanticweb.org/acontenti/ontologies/survival#StopBrazilGenocide
survival:StopBrazilGenocide rdf:type owl:NamedIndividual ,
                                     survival:Campaign ;
                            survival:directlyHelps survival:Yanomami ;
                            survival:ledBy survival:APIB ,
                                           survival:SurvivalBrasil .


###  http://www.semanticweb.org/acontenti/ontologies/survival#SurvivalBrasil
survival:SurvivalBrasil rdf:type owl:NamedIndividual ,
                                 survival:Organization ;
                        survival:branchOf survival:SurvivalInternational ;
                        survival:locatedIn survival:Brazil ;
                        survival:email "info@survivalbrasil.org"^^xsd:string ;
                        survival:website "https://www.survivalbrasil.org"^^xsd:anyURI .


###  http://www.semanticweb.org/acontenti/ontologies/survival#SurvivalInternational
survival:SurvivalInternational rdf:type owl:NamedIndividual ,
                                        survival:Organization ;
                               survival:locatedIn survival:UK ;
                               survival:email "info@survivalinternational.org" ;
                               survival:website "https://www.survivalinternational.org"^^xsd:anyURI ;
                               rdfs:label "Survival International Charitable Trust"@en .


###  http://www.semanticweb.org/acontenti/ontologies/survival#SurvivalItalia
survival:SurvivalItalia rdf:type owl:NamedIndividual ,
                                 survival:Organization ;
                        survival:branchOf survival:SurvivalInternational ;
                        survival:locatedIn survival:Italy ;
                        survival:email "info@survival.it"^^xsd:string ;
                        survival:website "https://www.survival.it"^^xsd:anyURI .


###  http://www.semanticweb.org/acontenti/ontologies/survival#SurvivalUSA
survival:SurvivalUSA rdf:type owl:NamedIndividual ,
                              survival:Organization ;
                     survival:branchOf survival:SurvivalInternational ;
                     survival:locatedIn survival:USA ;
                     survival:email "info.usa@survivalinternational.org"^^xsd:string ;
                     survival:website "https://www.survivalinternational.org"^^xsd:anyURI .


###  http://www.semanticweb.org/acontenti/ontologies/survival#UK
survival:UK rdf:type owl:NamedIndividual ,
                     survival:Country ;
            survival:locatedIn survival:Europe .


###  http://www.semanticweb.org/acontenti/ontologies/survival#USA
survival:USA rdf:type owl:NamedIndividual ,
                      survival:Country ;
             survival:locatedIn survival:NorthAmerica .


###  http://www.semanticweb.org/acontenti/ontologies/survival#UncontactedFrontier
survival:UncontactedFrontier rdf:type owl:NamedIndividual ,
                                      survival:Habitat ;
                             survival:locatedIn survival:Bolivia ,
                                                survival:Brazil ,
                                                survival:Peru ;
                             survival:habitatType "forest" .


###  http://www.semanticweb.org/acontenti/ontologies/survival#UncontactedTribes
survival:UncontactedTribes rdf:type owl:NamedIndividual ,
                                    survival:Campaign ;
                           survival:addresses survival:ForcedContact .


###  http://www.semanticweb.org/acontenti/ontologies/survival#Yanomami
survival:Yanomami rdf:type owl:NamedIndividual ,
                           survival:Tribe ;
                  survival:livesIn survival:YanomamiPark ;
                  survival:threatenedBy survival:Deforestation ,
                                        survival:IllegalMining ;
                  survival:contacted "false"^^xsd:boolean .


###  http://www.semanticweb.org/acontenti/ontologies/survival#YanomamiPark
survival:YanomamiPark rdf:type owl:NamedIndividual ,
                               survival:Habitat ;
                      survival:locatedIn survival:Brazil ;
                      survival:habitatType "forest" .


#################################################################
#    General axioms
#################################################################

